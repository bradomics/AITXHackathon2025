#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules/texture-compressor/bin/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules/texture-compressor/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules/texture-compressor/bin/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules/texture-compressor/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/texture-compressor@1.0.2/node_modules:/Users/brad/Projects/Web/AITXHackathon2025/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../texture-compressor/bin/texture-compressor.js" "$@"
else
  exec node  "$basedir/../texture-compressor/bin/texture-compressor.js" "$@"
fi
